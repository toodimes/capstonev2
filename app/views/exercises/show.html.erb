<h1>Exercises#show</h1>
<p>Find me in app/views/exercises/show.html.erb</p>
<h3><%= @exercise.name %></h3>
<p><a href="/">Muscle: <%= @exercise.muscle.name %></a></p>
<% @exercise.exercise_images.each do |gif| %>
  <img src="<%= gif.url %>" alt="">
<% end %>
<% @exercise.descriptions.each do |desc| %>
  <p><%= desc.note %></p>
<% end %>