<div id="messages-index-app">
  <%= render "layouts/breadcrumbs" %>
  <div class="container">
    <div class="row">
      <div class="col-md-8 unread">
        <h3>Connections</h3>
        <hr>
        <h2 v-for="name in allNames"><a :href="'/messages/' + name[1]">{{ name[0] }}</a>  <img class="img-circle message-img" :src="name[2]" alt=""></h2>
      </div>
      <div class="col-md-4">
        <h3><a href="/messages/new">New Connection</a></h3>
        <hr>
      </div>
    </div>
  </div>
</div>
